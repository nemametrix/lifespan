{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "/home/image_server/lifespan/build/ns_image_server",
            "args": [],
            "stopAtEntry": true,
            "cwd": ".",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "pipeTransport": {
                "debuggerPath": "/usr/bin/gdb",
                "pipeProgram": "docker",
                "pipeArgs": [
                    "exec",
                    "-i",
                    "lifespan",
                    "sh",
                    "-c"
                ],
                "pipeCwd": ""
            },
            "sourceFileMap": {
                "/home/image_server/lifespan": "${cwd}"
            },
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}